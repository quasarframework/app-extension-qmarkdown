    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^4.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-quasar": "^1.0.0-alpha.18",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.0",
    "node-sass": "^4.14.1",
    "open": "^7.0.4",
    "postcss": "^7.0.30",
    "postcss-rtl": "^1.7.3",
